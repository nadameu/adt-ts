{
  "name": "pure-ts",
  "version": "1.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@nadameu/pipe": "^2.0.0",
    "@types/jest": "^24.0.17",
    "jest": "^24.8.0",
    "jsverify": "^0.8.4",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "rollup": "^1.19.4",
    "rollup-plugin-typescript2": "^0.22.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "clean": "rimraf dist .rpt2_cache",
    "build-cjs": "tsc -p tsconfig-cjs.json",
